{
  "name": "@erikwithuhk/html-to-jsx",
  "version": "1.0.1-2",
  "description": "Converts HTML to JSX for use with React",
  "main": "/lib/index.js",
  "files": [
    "/lib/**/*.js"
  ],
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib --copy-files",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib",
    "lint": "eslint ./src",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikwithuhk/html-to-jsx.git"
  },
  "keywords": [
    "react",
    "jsx",
    "htmltojsx",
    "html",
    "react-component",
    "parser"
  ],
  "author": "Daniel Lo Nigro <daniel@dan.cx> (http://dan.cx/)",
  "contributors": [
    "erikwithuhk"
  ],
  "maintainers": [
    "erikwithuhk"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/reactjs/react-magic/issues"
  },
  "homepage": "https://github.com/reactjs/react-magic",
  "dependencies": {
    "jsdom-no-contextify": "~3.1.0",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "yargs": "~4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": ">=4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2"
  }
}
